@model FabricErrorException
@if ( Model.RespError != null ) {
	<h1>FabricErrorException</h1>
	<h2>@Model.RespError.Name</h2>
	<p>
		@Model.RespError.Message<br />
		<br />
		(Code: @Model.RespError.Code)
	</p>
}
else {
	<h1>FabricErrorException (OAuth)</h1>
	<h2>@Model.OauthError.error</h2>
	<p>
		@Model.OauthError.error_description
	</p>
}